  "homepage": "https://github.com/HiDeoo/starlight-blog",
  "repository": {
    "type": "git",
    "url": "https://github.com/HiDeoo/starlight-blog.git"
  },
  "bugs": "https://github.com/HiDeoo/starlight-blog/issues",
  "lint-staged": {
    "**/*": "prettier -w -u --cache",
